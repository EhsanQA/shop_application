<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #5EBA9A;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.services.costumers.costumers.CustomerLandingPage">
   <children>
      <Button fx:id="logOutBtn" layoutX="33.0" layoutY="345.0" mnemonicParsing="false" onAction="#logOut" style="-fx-background-color: #FF5D00;" text="Logout" />
